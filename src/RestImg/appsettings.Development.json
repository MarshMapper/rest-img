{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Caching": {
    // configure caching of the original files that will be resized
    "ImageFileCache": {
      "Enabled": true,
      "TimeoutInSeconds": 300
    },
    // configure caching of the resized images using .NET Output Cache
    "OutputCache": {
      "Enabled": true,
      "TimeoutInSeconds": 300
    }
  }
}
